generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type LinksTypeProps {
  links TechsTypeProps[]
}

type TechsTypeProps {
  id       String
  svg_name String
  link     String
  svg_link String
}

model Project {
  id          String         @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  link_deploy String
  banner      String
  thumbnail   String
  description String
  techs       LinksTypeProps

  created_at DateTime @default(now())

  @@map("projects")
}